{
	"name": "sectionExtension",
	"version": "0.1.0",
	"author": [
		"Anthony Ha"
	],
	"url": "https://github.com/alfaha/sectionExtension.git/",
	"descriptionmsg": "sectionextension-desc",
	"license-name": "GPL v2",
	"type": "semantic",
	"AutoloadClasses": {
		"sectionExtensionHooks": "sectionExtension.hooks.php",
		"SpecialHelloWorld": "specials/SpecialHelloWorld.php"
	},
	"config": {
		"sectionExtensionEnableFoo": true
	},
	"ExtensionMessagesFiles": {
		"sectionExtensionAlias": "sectionExtension.i18n.alias.php"
	},
	"Hooks": {
		
		"ParserFirstCallInit": [
			"sectionExtensionHooks::onParserFirstCallInit"
		]
	},
	"MessagesDirs": {
		"sectionExtension": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.sectionExtension.foo": {
			"scripts": [
				"modules/ext.sectionExtension.js",
				"modules/ext.sectionExtension.foo.js"
			],
			"styles": [
				"modules/ext.sectionExtension.foo.css"
			],
			"messages": [],
			"dependencies": []
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "sectionExtension"
	},
	"SpecialPages": {
		"HelloWorld": "SpecialHelloWorld"
	},
	"manifest_version": 1
}
